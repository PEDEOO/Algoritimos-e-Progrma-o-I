                <then>
                    <assign variable="ideal" expression="(72.7 * h) - 58"/>
                </then>
                <else>
                    <assign variable="ideal" expression="(62.1 * h) - 44.7"/>
                </else>
            </if>
            <output expression="&quot;O peso ideal sera de: &quot; &amp;ideal" newline="True"/>
        </body>
