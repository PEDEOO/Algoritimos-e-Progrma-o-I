    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="h, peso" type="Integer" array="False" size=""/>
            <declare name="ideal" type="Real" array="False" size=""/>
            <declare name="sexo" type="String" array="False" size=""/>
            <output expression="&quot;Insira o seu sexo&quot;" newline="True"/>
            <input variable="sexo"/>
            <output expression="&quot;Insira a sua altura&quot;" newline="True"/>
            <input variable="h"/>
